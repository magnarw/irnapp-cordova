<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.irnapp712539" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>IrnApp</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="hi@ionicframework" href="http://ionicframework.com/">
      Ionic Framework Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="AutoHideSplashScreen" value="false" />
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
 <gap:plugin name="de.appplant.cordova.plugin.local-notification" />
 
<preference name="StatusBarStyle" value="blackopaque" />

  <platform name="ios">
      
      <!-- iOS 7.0+ -->
      <!-- iPhone / iPod Touch -->
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="60" height="60" />
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="120" height="120" />
      
      <!-- iPad -->
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="76" height="76" />
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="152" height="152" />
      
      <!-- iOS 6.1 -->
      <!-- Spotlight Icon -->
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="40" height="40" />
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="80" height="80" />
      
      <!-- iPhone / iPod Touch -->
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="57" height="57" />
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="114" height="114" />
      
      <!-- iPad -->
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="72" height="72" />
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="144" height="144" />
      
      <!-- iPhone Spotlight and Settings Icon -->
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="29" height="29" />
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="58" height="58" />
      
      <!-- iPad Spotlight and Settings Icon -->
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="50" height="50" />
      <icon src="www/icons/drawable-xxhdpi/ic_launcher_white.png" width="100" height="100" />
  </platform>
</widget>